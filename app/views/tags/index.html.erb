<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag), class: 'tag_title', id: "tag_#{tag.id}" %>
      <!-- <a href="/tags/1">First Sample tag</a> -->
    </li>
  <% end %>
</ul>

<%= link_to "Create a New tag", new_tag_path, class: 'new_tag' %>
